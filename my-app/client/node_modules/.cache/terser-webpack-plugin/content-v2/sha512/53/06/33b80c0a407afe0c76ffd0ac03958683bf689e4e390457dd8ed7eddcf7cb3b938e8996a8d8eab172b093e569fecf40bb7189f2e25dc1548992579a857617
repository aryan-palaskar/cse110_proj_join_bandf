\nimport './PinPageStyle.css';\r\n\r\nfunction PinPage() {\r\n  const [lobbyCode, setLobbyCode] = useState('');\r\n  const [showError, setShowError] = useState(false);\r\n\r\n  const handleLobbyCodeChange = (e: React.ChangeEvent<HTMLInputElement>) => {\r\n    const numericInput = e.target.value.replace(/\\D/g, '').slice(0, 4); // Limit to 4 digits\r\n    setLobbyCode(numericInput);\r\n  };\r\n\r\n  const handleJoinGame = async () => {\r\n    try {\r\n      const response = await fetch('http://localhost:5000/api/join', {\r\n        method: 'POST',\r\n        headers: {\r\n          'Content-Type': 'application/json',\r\n        },\r\n        body: JSON.stringify({\r\n          lobbyId: 1, // Use the actual lobby ID or pass it dynamically\r\n          userId: 'playerUser', // Replace with the actual user ID or prompt for it\r\n          pin: lobbyCode,\r\n        }),\r\n      });\r\n\r\n      const data = await response.json();\r\n\r\n      if (response.ok) {\r\n        console.log('Lobby code is valid. Proceeding to join game...');\r\n        setShowError(false); // Hide error if PIN is correct\r\n        // Redirect to game or update the UI\r\n      } else {\r\n        console.error(data.error);\r\n        setShowError(true); // Show error if PIN is incorrect\r\n      }\r\n    } catch (error) {\r\n      console.error('Error joining the game:', error);\r\n      setShowError(true);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"pin-page\">\r\n      <header className=\"header\">\r\n        <h1>Enter Lobby Code</h1>\r\n        <input\r\n          type=\"text\"\r\n          value={lobbyCode}\r\n          onChange={handleLobbyCodeChange}\r\n          placeholder=\"Enter 4-digit PIN\"\r\n        />\r\n        <button onClick={handleJoinGame}>Join Game</button>\r\n      </header>\r\n\r\n      {showError && <div className=\"error\">Invalid PIN. Please try again.</div>}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default PinPage;\r\n","import React from 'react';\r\ni